@{
    ViewBag.Title = "Página da Administração";
}

<img id="logo_oficial" src="../Images/logooficial.png" />

@section head{

        <script type="text/javascript">
            /*<![CDATA[*/
            $(function () {
                $.fn.vAlign = function () {
                    return this.each(function () {
                        $(".floater").css('margin-top', -Math.ceil($(this).height() / 2) + "px");
                    });
                };

                $(".center").vAlign();
            });
            /*]]>*/
        </script>

}